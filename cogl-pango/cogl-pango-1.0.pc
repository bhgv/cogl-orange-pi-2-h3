apiversion=1.0
requires= cairo >= 1.10 gobject-2.0 gmodule-no-export-2.0 gdk-pixbuf-2.0 >= 2.0 wayland-egl >= 1.0.0 wayland-client >= 1.0.0 egl x11 xext xfixes >= 3 xdamage xcomposite >= 0.4 xrandr >= 1.2 cogl-1.0

